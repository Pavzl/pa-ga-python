    "import csv\n",
    "\n",
    "total_spent = 0\n",
    "\n",
    "with open(\"uber_eats.csv\") as data_file:\n",
    "    data = csv.reader(data_file)\n",
    "    for row in data:\n",
    "        if row[2] != \"price\":\n",
    "            total_spent += float(row[2])\n",
    "\n",
    "print(total_spent)\n",
    "        \n",
    "        \n",
    "        "
   ]
  }
